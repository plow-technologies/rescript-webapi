{
  "name": "rescript-webapi",
  "version": "0.1.0",
  "description": "Reason / ReScript bindings to DOM",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinymce/rescript-webapi.git"
  },
  "homepage": "https://github.com/tinymce/rescript-webapi#readme",
  "keywords": [
    "reason",
    "bucklescript",
    "rescript",
    "web",
    "dom"
  ],
  "scripts": {
    "build": "rescript build",
    "start": "rescript build -w",
    "clean": "rescript clean",
    "doc-install": "npm install --no-save bsdoc",
    "test": "rescript build && bsdoc build api && bsdoc support-files",
    "refmt-all": "find src tests \\( -name '*.re' -o -name '*.rei' \\) | xargs bsrefmt -w 100 --in-place"
  },
  "files": [
    "src",
    "bsconfig.json"
  ],
  "author": "Tiny Technologies Inc",
  "license": "MIT",
  "devDependencies": {
    "rescript": "^9.1.4"
  },
  "dependencies": {
    "bs-fetch": "plow-technologies/bs-fetch#960d284ebcffc0d097c164edec428c1bd80da53b"
  }
}
